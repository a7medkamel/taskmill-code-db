{
  "git" : {
    "hook_url" : "https://www.breadboard.io/githook",
    "hosts" : [
      {
        "hostname" : "github.com",
        "hook": {
          "secret" : "[SECRET]"
        },
        "options" : {
          "debug": false,
          "protocol": "https",
          "host": "api.github.com",
          "pathPrefix": null,
          "headers": {
            "user-agent": "[USER_AGENT]"
          },
          "timeout": 5000
        }
      }
    ]
  },
  "codedb"  : {
    "winston"       : "info",
    "port"          : "8585",
    "ttl"           : 86400,
    "populate"      : {
      "limit"       : 100
    },
    "hook" : {
      "clean" : false
    },
    "redis" : {
      "host"          : "[HOST]",
      "port"          : 6379,
      "password"      : "[PASSWORD]",
      "db"            : 3
    },
    "pull-on-start" : true
  },
  "account"             : {
    "url"               : "http://account.breadboard.io"
  },
  "pubsub" : {
    "host"          : "[HOST]",
    "port"          : 6379,
    "password"      : "[PASSWORD]",
    "db"            : 1
  }
}
